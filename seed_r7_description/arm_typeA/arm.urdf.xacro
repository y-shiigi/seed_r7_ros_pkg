<?xml version="1.0"?>
<robot name="SEED-Arm-Mover-typeA" xmlns:xacro="http://ros.org/wiki/xacro">

  <!-- The following included files set up definitions of parts of the robot body -->
  <!-- wraps upperbody and hand-->
  <xacro:include filename="$(find seed_r7_description)/arm_typeA/arm-upper.urdf.xacro" />
  <!-- wraps lifter and wheels-->
  <xacro:include filename="$(find seed_r7_description)/arm_typeA/noid-wheels.urdf.xacro" />
 
 <!-- Now we can start using the macros included above to define the actual robot -->
 <xacro:arm-upper/>
 <xacro:noid-wheels base_link="base_link"/>
</robot>